    "from datetime import datetime\n",
    "\n",
    "# 获取当前时间\n",
    "current_datetime = datetime.now()\n",
    "\n",
    "# 将datetime对象格式化为字符串\n",
    "current_datetime_str = current_datetime.strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "\n",
    "# 打印格式化后的时间字符串\n",
    "print(current_datetime_str)\n"
